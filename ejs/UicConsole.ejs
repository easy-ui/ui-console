        "$('').show();",
        "$('').size();",
        "$('').slideDown();",
        "$('').slideToggle();",
        "$('').slideUp();",
        "$('').stop();",
        "$('').submit();",
        "$('').text('');",
        "$('').toArray();",
        "$('').toggle();",
        "$('').toggleClass('');",
        "$('').trigger('');",
